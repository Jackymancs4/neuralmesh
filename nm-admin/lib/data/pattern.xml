<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queries[]>
<queries>
	<query name="removeAll">
	<![CDATA[
	DELETE FROM patterns
	WHERE setID = :id
	]]>
	</query>
	
	<query name="getAll">
	<![CDATA[
	SELECT *
	FROM patterns
	WHERE setID = :id
	]]>
	</query>
	
	<query name="remove">
	<![CDATA[
	DELETE FROM patterns
	WHERE patternID = :id
	]]>
	</query>
	
	<query name="add">
	<![CDATA[
	INSERT INTO patterns(setID,pattern,output)
	VALUES (:id,:pattern,:output)
	]]>
	</query>
	
	<query name="getSet">
	<![CDATA[
	SELECT * 
	FROM sets t INNER JOIN patterns p ON p.setID = t.setID 
	WHERE t.setID = :id
	]]>
	</query>
</queries>